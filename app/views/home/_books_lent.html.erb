<div class="row-fluid">
	<div class="col-md-12">
		<h3 class="title_books">Books I lent</h3> 
		<table class="table table-striped table-bordered">
		  <thead>
		    <tr>
		      <th class="title_books">Title</th>
		      <th class="title_books">Author</th>
		      <th class="title_books">User</th>
		    </tr>
		  </thead>
		  <tbody>
		  	<% unless book.nil? %>
		    <% book.each do |b| %>
		      <tr>
		        <td><%= b.book.title %></td>
		        <td><%= b.book.author %></td>
		        <td>
		          <%= b.user.email %>  
		        </td>
		      </tr>
		    <% end %>
		    <% end %>
		  </tbody>
		</table>
	</div>
</div>
